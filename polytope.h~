#include <iostream>
#include "lattice.h"

class Polytope
{
    private:
    int vertices;
    int nrOfVertices;  
    Lattice* lattice; 

    public:

    void setVertices(int mVertices);
    void setLattice(Lattice* lattice);
    int getVertices();
    int getNrOfVertices();
    Lattice* getLattice();
    void drawPolytope();
};

